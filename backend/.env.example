# Webhook signing secret (Dashboard > Integrations > Webhooks > after creating webhook)
REVENUECAT_WEBHOOK_SECRET=your-revenuecat-webhook-secret

# Subscription Configuration
FREE_TIER_WEEKLY_MESSAGE_LIMIT=20
